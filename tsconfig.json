{
  "compilerOptions": {
    "rootDirs": [
      "./src",
    ],
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "types": ["node", "jest", "express", "cypress"]
  },
  "include": [
    "src/**/*",
    "cypress/**/*",
    "../node_modules/cypress"
  ]
}